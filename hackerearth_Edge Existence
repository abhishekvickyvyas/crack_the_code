#include<iostream>
using namespace std;
int main()
 {
     int a,b,i,j,N,M,Q,l,m;
     cin >>N>>M;
      int ar[N][N]={0};
      for(i=0;i<M;i++)
       {   
           
           cin>>a>>b;
           ar[a][b]=1;
           ar[b][a]=1;
       }
       cin>>Q;
       for(i=0;i<Q;i++)
         {
             cin>>l>>m;
             if(ar[l][m]==1)
             cout<<"YES\n";
             else
             cout<<"NO\n";
             
         }
      return 0;
 }
